/*         
 1 5 9  13 
 2 6 10 14 
 3 9 11 15 
 4 8 12 16 
*/
class Number_pattern6
{
	public static void main(String[] args) 
	{
		int n=4;
		
		for (int i=1 ; i<=n ; i++ )
		{
			int k=i;
			for (int j=1 ; j<=n ; j++ )
			{
				if (k<=9)
				{
					System.out.print(" ");
				}
				System.out.print(" "+k);
				k=k+n;
			}
			System.out.println(" ");
		}
	}
}
