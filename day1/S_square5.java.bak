class S_square5
{
	public static void main(String[] args) 
	{
		int n=4;
		for (int a=1; a<=n;a++ )
		{
			for (int b=1;b<=n ;b++ )
			{   
				if (a==2 && b==2 || a==3 && b==3)
				{
					System.out.print("  ");
				}
				else
				{
					System.out.print("* ");
				}
			}
			System.out.println();
		}
	}
}
