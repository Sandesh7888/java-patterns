package PatternNumber;
/*   
     
* * * * 1 =>row
*     *
*     *
* * * * 4

*/
class S_square2
{
	public static void main(String[] args) 
	{
		int n=4;
		for (int a=1; a<=n;a++ )
		{
			for (int b=1;b<=n ;b++ )
			{
				if (a==1 || a==4  && b==1|| b==4)
				{
					System.out.print("* ");
				}
				else
				{
					System.out.print("  ");
				}
				
			}
			System.out.println();
		}
	}
}
